<!--================Contact Area =================-->
<section class="contact_area p_120" style="margin-top: 20px">
    <div class="container">
        <div class="row">
            <div class="card offset-md-4">
                <div class="card-header text-center">
                    <img src="img/logomarca.png" width="200px" alt="">
                    <br>
                    <br>
                    <h4 class="title_color">Fluilizer</h4>
                </div>
                <div class="card-body row">
                    <div class="col-lg-12">
                        <form class="row" action="/login" method="post" id="loginForm"
                            novalidate="novalidate">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <input type="email" class="form-control" id="email" name="email"
                                        placeholder="Enter email address">
                                </div>
                                <div class="form-group">
                                    <input type="password" class="form-control" id="password" name="password"
                                        placeholder="Enter password">
                                    <a href="" class="text-left">Esqueceu a senha?</a>
                                </div>
                            </div>
                            <div class="col-md-12 text-center">
                                <button type="submit" value="submit" class="btn submit_btn">Entrar</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--================Contact Area =================-->