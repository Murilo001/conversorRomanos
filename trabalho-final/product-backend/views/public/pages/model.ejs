<section class="contact_area p_120" style="margin-top: 20px">
    <div class="container">
        <div class="row">
            <div class="card offset-md-4">
                <div class="card-header text-center">
                    <img src="img/logomarca.png" width="200px" alt="">
                    <br>
                    <br>
                    <h4 class="title_color">Model Class</h4>
                </div>
                <div class="card-body row">
                    <div class="col-lg-12">
                        <form class="row" action="/modelGen" method="post" id="generateForm"
                            novalidate="novalidate">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <input type="nomeClasse" class="form-control" id="nomeClasse" name="nomeClasse"
                                        placeholder="Nome da Classe">
                                </div>
                                <div class="form-group">
                                    <input type="autor" class="form-control" id="autor" name="autor"
                                        placeholder="Autor">
                                </div>
                                <div class="form-group">
                                    <input type="atributos" class="form-control" id="atributos" name="atributos"
                                        placeholder="Atributos">
                                </div>
                            </div>
                            <div class="col-md-12 text-center">
                                <button type="submit" value="submit" class="btn submit_btn">Gerar Classe</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
